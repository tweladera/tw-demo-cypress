# Variables de entorno para Cypress OrangeHRM Tests
# Copia este archivo a .env y configura tus valores

# Credenciales de OrangeHRM Demo
ORANGEHRM_USERNAME=Admin
ORANGEHRM_PASSWORD=admin123

# Configuración de Cypress (opcional)
CYPRESS_BASE_URL=https://opensource-demo.orangehrmlive.com/web/index.php
CYPRESS_DEFAULT_COMMAND_TIMEOUT=10000
CYPRESS_REQUEST_TIMEOUT=10000

# Configuración de reportes (opcional)
CYPRESS_REPORTER=cypress-mochawesome-reporter
CYPRESS_REPORTER_OPTIONS=charts:true,reportPageTitle:'Cypress Report Test',embeddedScreenshots:true,inlineAssets:true 